{
  "name": "auto-type-ucode",
  "displayName": "auto-type-ucode",
  "description": "Auto Type Code is a Visual Studio Code extension that simulates real-time code typing into the editor.",
  "publisher": "mkswebs",
  "repository": "https://github.com/mukesh303/autotype-code.git",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
  "auto-type",
  "auto typer",
  "typewriter effect",
  "simulate typing",
  "editor automation",
  "vscode typing",
  "start typing from line",
  "skip lines",
  "code presentation",
  "demo typing",
  "typing animation",
  "live coding",
  "cursor jump",
  "editor control"
],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autoTypeReplay.start",
        "title": "Auto Type Replay: Start"
      }
    ],
    "keybindings": [
      {
        "command": "autoTypeReplay.start",
        "key": "ctrl+alt+k",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Auto Type Replay",
      "properties": {
        "autoTypeReplay.speed": {
          "type": "number",
          "default": 50,
          "description": "Typing speed in characters per second"
        },
        "autoTypeReplay.sourceFile": {
          "type": "string",
          "default": "replay-code.txt",
          "description": "File path to source code for replay"
        },
        "autoTypeReplay.startLine": {
          "type": "number",
          "default": 0,
          "description": "Starting line of auto typing"
        },
        "autoTypeReplay.sourceSkipLine": {
          "type": "number",
          "default": 0,
          "description": "Skipping lines from source file of auto typing"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
